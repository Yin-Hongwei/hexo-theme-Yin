header#page-header
  nav#navbar
    span.nav-left
      div.tou-img
        img(src="/img/tou.jpg")
      a#site-name(href=url_for('/')) #[=config.title]
    i.fa.fa-bars.toggle-menu.nav-right(aria-hidden="true")

    span.nav-right.menus
      each url, label in theme.menu
        a.site-page(href=url)= label
